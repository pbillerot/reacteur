{
  "name": "reacteur",
  "version": "1.0.0",
  "description": "Experimental Web application with React",
  "main": "index.js",
  "scripts": {
    "start": "NODE_ENV=production node_modules/.bin/babel-node --presets 'react,es2015' app/server.js",
    "start-dev": "npm run start-dev-hmr",
    "start-dev-single-page": "node_modules/.bin/http-server app/static",
    "start-dev-hmr": "node_modules/.bin/webpack-dev-server --progress --inline --hot",
    "build": "NODE_ENV=production node_modules/.bin/webpack -p"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pbillerot/reacteur.git"
  },
  "author": "pbillerot",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/pbillerot/reacteur/issues"
  },
  "homepage": "https://github.com/pbillerot/reacteur#readme",
  "dependencies": {
    "babel-cli": "^6.11.4",
    "babel-core": "^6.13.2",
    "babel-loader": "^6.2.5",
    "babel-plugin-react-html-attrs": "^2.0.0",
    "babel-preset-es2015": "^6.13.2",
    "babel-preset-react": "^6.11.1",
    "babel-preset-react-hmre": "^1.1.1",
    "body-parser": "^1.15.2",
    "ejs": "^2.5.1",
    "express": "^4.14.0",
    "form-data": "^2.1.2",
    "fs": "0.0.1-security",
    "jfs": "^0.2.6",
    "multer": "^1.2.0",
    "react": "^15.3.1",
    "react-dom": "^15.3.1",
    "react-remarkable": "^1.1.1",
    "react-router": "^2.6.1",
    "sqlite3": "^3.1.8",
    "winston": "^2.3.0"
  },
  "devDependencies": {
